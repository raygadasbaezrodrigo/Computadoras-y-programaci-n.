//Programa que pida 3 numeros enteros diferentes, estos deben ser mayores que cero y despliegue en pantalla cu√°l es mayor//
#include<stdio.h>
int main ()
{
    int num1=0;
    int num2=0;
    int num3=0;
    printf ("Dame el primer numero");
    scanf("%d",&num1);
    printf("Dame el segundo numero");
    scanf("%d",&num2);
    printf("Dame el tercer numero");
    scanf("%d",&num3);
    if (num1!=0&&num2!=0&&num3!=0)
    {
        if (num1>num2&&num1>num3)
         {
            printf ("%d es mayor",num1);
            }
        else
        {
            if (num2>num1&&num2>num3)
            printf("%d es mayor",num2);
            else 
            printf("%d es mayor",num3);
        }
    }
    else 
    printf ("error");
}
